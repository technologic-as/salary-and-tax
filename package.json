    "webpack-dev-server": "3.1.x",
    "webpack-merge": "4.1.x"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "yarn lint --fix",
      "git add"
    ],
    "*.{json,md,scss,yaml,yml}": [
      "yarn prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    }
  },
  "snyk": true
