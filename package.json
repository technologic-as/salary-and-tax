    "redux": "4.0.x",
    "redux-form": "7.4.x",
    "redux-form-storybook": "1.0.x",
    "redux-thunk": "2.3.x",
    "request": "2.88.x",
    "request-promise": "4.2.x",
    "typeface-roboto": "0.0.x",
    "snyk": "^1.316.1"
